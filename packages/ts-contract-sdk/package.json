{
  "name": "@truth-core/contract-sdk",
  "version": "0.2.0",
  "description": "TypeScript SDK for truth-core contracts - typed models and validators for Verdict and Readiness artifacts",
  "keywords": [
    "truth-core",
    "verification",
    "contracts",
    "typescript",
    "sdk",
    "ci",
    "testing",
    "quality-assurance"
  ],
  "homepage": "https://github.com/your-org/truth-core#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/truth-core.git",
    "directory": "packages/ts-contract-sdk"
  },
  "license": "MIT",
  "author": "Truth Core Contributors",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./helpers": {
      "types": "./dist/helpers.d.ts",
      "import": "./dist/helpers.js",
      "require": "./dist/helpers.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/types.ts src/helpers.ts --format esm,cjs --dts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
